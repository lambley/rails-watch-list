<div class="container">
<h1>Your Watch List</h1>
<h2>Your Lists</h2>
  <% @lists.each do |list| %>
    <div>
      <p><%= link_to list.name, list_path(list) %>
      | <%= link_to "delete list", list_path(list),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %></p>
    </div>
  <% end %>
<%= link_to "add new list", new_list_path %>
<%= link_to "add new bookmark", new_bookmark_path %>
</div>
